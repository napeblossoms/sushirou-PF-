<h1>新規投稿</h1>
<%= form_with model: @postsushi do |f| %>

  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %><br>

  <%= f.select :region, options_for_select(PostSushi.regions), {prompt: "選択してください"}, class: "prefecture-select" %>地方


  <h4>店名</h4>
  <%= f.text_field :name %>



  <h4>県</h4>
 <%= f.select :prefecture, options_for_select(PostSushi.prefectures), {prompt: "選択してください"}, class: "prefecture-select" %><br>
 <%= f.submit '投稿' %>

<% end %>